{
    "manifest_version": 3,
    "name": "Leetcode Code Formatter",
    "description": "Code Format Extension",
    "version": "1.0",
    "permissions": ["activeTab","scripting"],
    "icons":{
      "16":"extension/images/icon.png"
    },
    "content_scripts": [
      {
        "js": ["./extension/scripts/content.js"],
        "matches": [
          "https://*.leetcode.com/*",
          "https://*.leetcode.cn/*"
        ]
      }
    ]
  }